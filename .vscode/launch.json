    {
        "name": "(gdb) pfu",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/install/bin/pfu",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [
            { 
                "name" : "MUD_DATA_DIR",
                "value" : "${env:HOME}/tmp/pfiles"
            },
            { 
                "name" : "MUD_AREA_DIR",
                "value" : "${workspaceFolder}/install/area"
            },
            { 
                "name" : "MUD_HTML_DIR",
                "value" : "${workspaceFolder}/install/html"
            },
            {
                "name" : "MUD_PORT",
                "value" : "9000"
            }
        ],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
